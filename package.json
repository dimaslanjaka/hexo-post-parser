{
  "name": "hexo-post-parser",
  "projectName": "Hexo Post Parser",
  "version": "1.1.20-12-9cc452b",
  "description": "Parse Hexo Post To Object and pre-process all markdown posts for Automated SEO",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "ts-node build.ts",
    "postbuild": "node packer.js",
    "format": "prettier -w src/**/*.ts",
    "ci": "yarn install --immutable --immutable-cache --check-cache --frozen-lockfile --check-files",
    "update": "npx npm-check-updates -u -x chalk",
    "patch": "npm version patch",
    "postpatch": "npm run push",
    "push": "git push -u origin master --follow-tags"
  },
  "files": [
    "packages",
    "src",
    "dist",
    "_config.yml",
    "*.json",
    "postinstall.js",
    "preinstall.js"
  ],
