{
  "name": "hexo-post-parser",
  "projectName": "Hexo Post Parser",
  "version": "1.2.14",
  "description": "Parse Hexo Post To Object and pre-process all markdown posts for Automated SEO",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "clean": "rm -rf dist && tsc -b --clean",
    "build": "tsc --build tsconfig.build.json",
    "build-clean": "yarn clean && yarn build",
    "pack": "node packer.js --yarn",
    "docs": "node typedoc-runner.js --publish",
    "format": "prettier -w src/**/*.ts",
    "ci": "yarn install --immutable --immutable-cache --check-cache --frozen-lockfile --check-files",
    "update:postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js",
    "update:build-release": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/.github/workflows/build-release.yml > .github/workflows/build-release.yml",
    "update:typedoc": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/typedoc.js > typedoc.js",
    "update:typedoc-runner": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/typedoc-runner.js > typedoc-runner.js",
    "update:typedoc-server": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/typedoc-server.js > typedoc-server.js",
    "update:packer": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.js",
    "update:changelog": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/changelog.js > changelog.js",
    "update:ncu": "npx npm-check-updates -u -x chalk,jsdom,deepmerge-ts --enginesNode",
    "update": "run-s \"update:**\"",
    "test": "jest --runInBand"
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/hexo-post-parser.git"
  },
  "keywords": [
    "hexo",
    "markdown",
    "post",
    "parser",
    "hexo-parser",
    "hexo-post",
    "hexo-markdown"
  ],
  "author": "Dimas Lanjaka <dimaslanjaka@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimaslanjaka/hexo-post-parser/issues"
  },
  "homepage": "https://github.com/dimaslanjaka/hexo-post-parser#readme",
  "devDependencies": {
    "@jest/environment": "^29.7.0",
    "@jest/expect": "^29.7.0",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@types/bluebird": "^3.5.42",
    "@types/debug": "^4",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/gulp": "^4.0.17",
    "@types/hexo": "https://github.com/dimaslanjaka/hexo/raw/fc1f9b7/releases/hexo.tgz",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.14.202",
    "@types/lodash.deburr": "^4.1.9",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^20.11.17",
    "@types/showdown": "^2",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "binary-collections": "^2.0.5",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/raw/80999ac/release/cross-spawn.tgz",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "git-command-helper": "^2.0.2",
    "gulp": "^4.0.2",
    "hexo": "https://github.com/dimaslanjaka/hexo/raw/fc1f9b7/releases/hexo.tgz",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "jest-mock": "^29.7.0",
    "lint-staged": "^15.2.2",
    "minimatch": "^9.0.5",
    "npm-check-updated": "^6.0.4",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "semver": "^7.6.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.8",
    "typedoc-plugin-missing-exports": "^2.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "axios": "^1.6.7",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.2",
    "debug": "^4.3.4",
    "front-matter": "^4.0.2",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "highlight.js": "^11.9.0",
    "jsdom": "~21.1.2",
    "lodash": "^4.17.21",
    "lodash.deburr": "^4.1.0",
    "markdown-it": "^14.0.0",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-sub": "^2.0.0",
    "markdown-it-sup": "^2.0.0",
    "moment-timezone": "^0.5.45",
    "sbg-utility": "^1.1.6",
    "showdown": "^2.1.0",
    "tiny-typed-emitter": "^2.1.0",
    "true-case-path": "^2.2.1",
    "upath": "^2.0.1",
    "yaml": "^2.3.4",
    "yargs": "^17.7.2"
  },
  "directories": {
    "test": "test",
    "lib": "dist"
  },
  "packageManager": "yarn@4.5.0",
  "engines": {
    "node": ">=18"
  }
}
