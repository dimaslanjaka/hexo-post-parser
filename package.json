{
  "name": "hexo-post-parser",
  "projectName": "Hexo Post Parser",
  "version": "1.2.7",
  "description": "Parse Hexo Post To Object and pre-process all markdown posts for Automated SEO",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "yarn run prebuild && tsc -p tsconfig.build.json",
    "pack": "node packer.js --yarn",
    "docs": "node typedoc-runner.js --publish",
    "format": "prettier -w src/**/*.ts",
    "ci": "yarn install --immutable --immutable-cache --check-cache --frozen-lockfile --check-files",
    "update:postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js",
    "update:build-release": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/.github/workflows/build-release.yml > .github/workflows/build-release.yml",
    "update:typedoc": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/typedoc.js > typedoc.js",
    "update:typedoc-runner": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/typedoc-runner.js > typedoc-runner.js",
    "update:typedoc-server": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/typedoc-server.js > typedoc-server.js",
    "update:packer": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.js",
    "update:ncu": "npx npm-check-updates -u -x chalk",
    "update": "run-s update:** && node postinstall.js",
    "postupdate": "npm install",
    "test": "jest --runInBand"
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/hexo-post-parser.git"
  },
  "keywords": [
    "hexo",
    "markdown",
    "post",
    "parser",
    "hexo-parser",
    "hexo-post",
    "hexo-markdown"
  ],
  "author": "Dimas Lanjaka <dimaslanjaka@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimaslanjaka/hexo-post-parser/issues"
  },
  "homepage": "https://github.com/dimaslanjaka/hexo-post-parser#readme",
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/bluebird": "^3.5.38",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/gulp": "^4.0.10",
    "@types/hexo": "https://github.com/dimaslanjaka/nodejs-package-types/tarball/hexo",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.194",
    "@types/lodash.deburr": "^4.1.7",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.15.13",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/raw/private/release/cross-spawn.tgz",
    "debug": "^4.3.4",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "git-command-helper": "^1.1.9",
    "gulp": "^4.0.2",
    "jest": "^29.5.0",
    "minimatch": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "semver": "^7.5.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.4",
    "typedoc-plugin-missing-exports": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "axios": "^1.3.6",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.2",
    "front-matter": "^4.0.2",
    "fs-extra": "^11.1.1",
    "jsdom": "^21.1.1",
    "lodash": "^4.17.21",
    "lodash.deburr": "^4.1.0",
    "markdown-it": "^13.0.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "moment-timezone": "^0.5.43",
    "sbg-utility": "^1.0.9",
    "showdown": "^2.1.0",
    "tiny-typed-emitter": "^2.1.0",
    "true-case-path": "^2.2.1",
    "upath": "^2.0.1",
    "yaml": "^2.2.1",
    "yargs": "^17.7.1"
  },
  "directories": {
    "test": "test"
  },
  "packageManager": "yarn@3.5.0",
  "engineStrict": true,
  "engines": {
    "node": ">=16.0.0 <18.0.0"
  }
}
